<div class="movieframe">
<div class="movietitle">
  {{theatre.[0].Theatre_Name}}
</div>
<div class="moviedetails">
email:{{theatre.[0].Theatre_email}}<br>
Contact No:{{theatre.[0].Contact_No}}<br>
{{theatreloc.[0].Division}}
{{theatreloc.[0].Local_Body}}<br>
{{theatreloc.[0].District}}<br>
</div>
<div class="theatredetails">
  <table border="2" bordercolor="white" cellpadding="0" cellspacing="0" height="400" width="600">
   <tr>
     <th colspan="4">
       Screen details
     </th>
   </tr>
    <tr>
      <th>
        ScreenNo
      </th>
      <th>
        Screen Name
      </th>
      <th>
        Class Name
      </th>
      <th>
        Ticket cost
      </th>
    </tr>
    {{#each theatre2}}
    <tr align="center" border="2">
      <td>
        {{this.Screen_id}}
      </td>
      <td>
        {{this.Screen_Name}}
      </td>
      <td>
        {{this.Class_Name}}
      </td>
      <td>
        {{this.Cost}}
      </td>
    </tr>

    {{/each}}
    </table>
</div>
  
<div class="showdetails">

  <table border="2" bordercolor="white" cellpadding="0" cellspacing="0" height="400" width="600">
  <tr>
    <th colspan="3">Show details as on {{s}}</th>
  </tr>
  <tr>
  <th>
    Screen Name
  </th>
  <th>
    Show time
  </th>
  <th>
    Movie
  </th>
  </tr>
  {{#each theatre1}}
  <tr align="center" border="2">
  <td>
    {{this.Screen_Name}}
  </td>
  <td>
    {{this.Time}} {{this.Indication}}
  </td>
 <td>
   <a class="linkframe" href="/movieframe/{{this.Movie_id}}">
   <font Size="3">
        {{this.Movie_Name}}
   </font>
   </a>
 </td>
 
  </tr>
  {{/each}}
  </table>


</div>





</div>